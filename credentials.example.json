{
  "cloudflare": {
    "apiToken": "cf_api_token_here",
    "accountId": "cf_account_id_here",
    "workerName": "cf-webrtc-signaling"
  },
  "vars": {
    "ALLOW_DEV_TOKEN_ISSUER": "false",
    "TURN_URLS": "turn:turn.example.com:3478?transport=udp,turn:turn.example.com:3478?transport=tcp",
    "TURN_TTL_SECONDS": "3600",
    "TURN_RATE_LIMIT_MAX": "10",
    "TURN_RATE_LIMIT_WINDOW_SEC": "60"
  },
  "secrets": {
    "JOIN_TOKEN_SECRET": "replace-with-random-secret",
    "INTERNAL_API_SECRET": "replace-with-random-secret",
    "DEV_ISSUER_SECRET": "optional-dev-secret",
    "TURN_SHARED_SECRET": "optional-turn-secret"
  }
}
